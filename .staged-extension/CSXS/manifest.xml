<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="4.0" ExtensionBundleId="com.kelly.colorpicker" ExtensionBundleVersion="1.0.0"
		ExtensionBundleName="KellyColorPicker" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.kelly.colorpicker.extension" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PHSP" Version="[14.0,99.9]" />
			<Host Name="PHXS" Version="[14.0,99.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="4.0" />

		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.kelly.colorpicker.extension">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>KellyColorPicker</Menu>
					<Geometry>
						<Size>
							<Height>150</Height>
							<Width>180</Width>
						</Size>
						<MinSize>
							<Height>150</Height>
							<Width>150</Width>
						</MinSize>
					</Geometry>
					<Icons>
						<Icon Type="Normal">./icons/icon.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>